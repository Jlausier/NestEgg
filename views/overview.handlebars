     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
     <section>
        <div class="main-content flex-1 bg-gray-700 mx-10 mb-10 pb-24 md:pb-5 rounded-2xl bg-gray-700">
            {{!-- Title --}}
            <h1 class="rounded-t-3xl pl-12 pt-12 text-5xl text-gray-100">Overview</h1>

            {{!-- Content Container --}}
            <div class="p-6 flex flex-col bg-gray-700">

                {{!-- top portion --}}
                <div class="flex">

                    {{!-- Content Card 1 --}}
                    <div class="p-6 flex w-3/5">
                        <div class="bg-gray-800 rounded-lg shadow-xl w-full">
                            <h3 class="px-8 pt-6 pb-3 w-full border-b border-gray-600 tracking-wider uppercase text-left text-gray-200">Spending Overview</h3>
                            <div class="p-6 flex">
                                <div class="mx-6 w-2/5 ">
                                    {{> budget-overview-chart expenseData}}
                                </div>
                                <div class="flex flex-col items-start">
                                    <div class="mb-1 mt-6 text-left text-6xl font-bold text-gray-200">${{expenseData.total}}</div>
                                    <span class="mb-1 text-left text-2xl text-gray-400">Spent This Month</span>
                                    <a href="/budget" class="mt-4 px-3 py-1 border border-gray-400 rounded-full text-left text-md text-gray-400 hover:bg-gray-500 hover:border-gray-500 hover:text-gray-200 transition ease-in-out">Edit Spending</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    {{!-- Content Card 2 --}}
                    <div class="p-6 w-2/5 flex">
                        <div class="bg-gray-800 rounded-lg shadow-xl w-full">
                            <h3 class="px-8 pt-6 pb-3 w-full border-b border-gray-600 tracking-wider uppercase text-left text-gray-200">Monthly Comparison</h3>
                            <div class="p-6">
                                {{> monthly-overview-chart nonessentialData}}
                            </div>
                        </div>
                    </div>
                {{!-- top portion closer --}}
                </div>

                {{!-- Content Card 3 --}}
                <div class="p-6 w-full flex">
                    <div class="bg-gray-800 rounded-lg shadow-xl w-full">
                        <h3 class="px-8 pt-6 pb-3 w-full tracking-wider uppercase text-left text-gray-200">Goals</h3>
                        <div class="flex p-6">
                            {{> budget-table data=goalsData name="goalsOverview"}}
                        </div>
                    </div>
                </div>
            {{!-- Content Container Closer --}}
            </div>
        </div>
    </section>